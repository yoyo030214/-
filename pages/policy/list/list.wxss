.container {
  padding: 40rpx;
  min-height: 100vh;
  background: #f8f8f8;
}

.header {
  margin-bottom: 60rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}

.placeholder {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 60vh;
}

.placeholder-image {
  width: 300rpx;
  height: 300rpx;
  margin-bottom: 40rpx;
}

.placeholder-text {
  font-size: 28rpx;
  color: #999;
} 