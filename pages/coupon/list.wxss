.container {
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #f8f9fa;
}

.dev-tips {
  text-align: center;
  padding: 40rpx;
}

.dev-icon {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 40rpx;
  opacity: 0.8;
}

.dev-text {
  display: block;
  font-size: 32rpx;
  color: #666;
  margin-bottom: 20rpx;
}

.dev-subtext {
  font-size: 26rpx;
  color: #999;
} 